{
    "name": "zus1/laravel-base-repository",
    "description": "Base repository for laravel",
    "keywords": ["laravel", "base-repository", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Ožuška",
            "email": "mario.ozuska@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/pagination": "^11.0",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zus1\\LaravelBaseRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zus1\\LaravelBaseRepository\\Providers\\LaravelBaseRepositoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
